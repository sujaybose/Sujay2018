package collectionJava;

import java.util.ArrayList;


public class ArrayListLearn {

    public static void main(String[] args) {
    	
    	ArrayList ar= new ArrayList<>();
    	
    	ar.add(566);
    	ar.add("sujay");
    	ar.add(12.88);
    	
    	for (int i = 0; i < ar.size(); i++) {
    		System.out.println(ar.get(i));
			
		}
    	
    	
    }
}